{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "pulsar"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        "config.json"
      ],
      "index": "index.html",
      "main": "main.ts",
      "polyfills": "polyfills.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "ps",
      "styles": [
        "../node_modules/animate.css/animate.min.css",
        "../node_modules/bootstrap/dist/css/bootstrap.min.css",
        "../node_modules/froala-editor/css/froala_editor.pkgd.min.css",
        "../node_modules/froala-editor/css/froala_style.min.css",
        "../node_modules/fullcalendar/dist/fullcalendar.min.css",
        "../node_modules/quill/dist/quill.snow.css",
        "../node_modules/cropperjs/dist/cropper.min.css",
        "../node_modules/nanoscroller/bin/css/nanoscroller.css",
        "../node_modules/primeng/resources/primeng.min.css",
        "assets/vendor/flag-icon-css/css/flag-icon.css",
        "assets/vendor/font-awesome/scss/font-awesome.scss",
        "assets/vendor/jquery-ui/jquery-ui.min.css",
        "assets/vendor/jquery-ui/jquery-ui.structure.min.css",
        "assets/vendor/pace/pace.min.css",
        "assets/helpers/helpers.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/tether/dist/js/tether.min.js",
        "../node_modules/popper.js/dist/umd/popper.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "../node_modules/nanoscroller/bin/javascripts/jquery.nanoscroller.js",
        "../node_modules/froala-editor/js/froala_editor.pkgd.min.js",
        "../node_modules/moment/moment.js",
        "../node_modules/chart.js/dist/Chart.js",
        "../node_modules/fullcalendar/dist/fullcalendar.js",
        "../node_modules/quill/dist/quill.js",
        "../node_modules/cropperjs/dist/cropper.min.js",
        "assets/layout/js/ripple.js",
        "assets/vendor/jquery-ui/jquery-ui.min.js",
        "assets/vendor/pace/pace.min.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "component": {}
  }
}
